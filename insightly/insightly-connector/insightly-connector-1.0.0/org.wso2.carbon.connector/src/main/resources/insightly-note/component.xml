<component name="insightly-note" type="synapse/template">
   <subComponents>
      <component name="createNote">
         <file>createNote.xml</file>
         <description>Create a new note in Insightly account using the provided Parameters.</description>
      </component>
      <component name="getNote">
         <file>getNote.xml</file>
         <description>Retrieve details of a note identified by the provided note ID.</description>
      </component>
      <component name="listNotes">
         <file>listNotes.xml</file>
         <description>Retrieve a List of notes from Insightly account.</description>
      </component>
      <component name="updateNote">
         <file>updateNote.xml</file>
         <description>Update details of the note identified by the provided note ID.</description>
      </component>
   </subComponents>
</component>